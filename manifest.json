{
	"name": "9gag post filter",
	"description": "A Google Chrome extension!",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"storage"
	],
	"action": {
	  "default_popup": "index.html"
	},
	"content_scripts": [{
		"css": ["assets/css/content.css"],
		"js": ["assets/js/jquery_slim_mini.js","assets/js/content.js"],
		"matches": ["https://9gag.com/*"]
	  }]
}